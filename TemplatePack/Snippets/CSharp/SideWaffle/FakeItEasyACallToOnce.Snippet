<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Author>Christian Fredh - http://www.christianfredh.com</Author>
      <Description>Adds a faked method return from a fake.</Description>
      <HelpUrl>https://github.com/FakeItEasy/FakeItEasy/wiki/Creating-Fakes</HelpUrl>
      <Keywords>
        <Keyword>C#</Keyword>
        <Keyword>FakeItEasy</Keyword>
      </Keywords>
      <Shortcut>acalltoonce</Shortcut>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>FakeItEasy - Call To Must Have Happened Once</Title>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>fake</ID>
          <ToolTip>The fake.</ToolTip>
          <Default>fake</Default>
        </Literal>
        <Literal>
          <ID>method</ID>
          <ToolTip>Method to fake.</ToolTip>
          <Default>Method()</Default>
        </Literal>
      </Declarations>
      <Code Language="CSharp">
        <![CDATA[A.CallTo(() => $fake$.$method$).MustHaveHappened(Repeated.Exactly.Once);$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>